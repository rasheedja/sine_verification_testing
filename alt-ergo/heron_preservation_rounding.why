logic eps : real

goal heron_preservation_rounding :
  forall x:real.
    forall y:real.
      0.5 <= x <= 2.0 -> 0.8 <= y <= 1.8 -> eps = 1.0/8388608.0 ->
        ((y + x/y) / 2.0) - (y + (x/y * (1.0 - eps)) * (1.0 - eps)) / 2.0 * (1.0 - eps) <= 6.0 * eps and
        ((y + x/y) / 2.0) - (y + (x/y * (1.0 - eps)) * (1.0 - eps)) / 2.0 * (1.0 - eps) >= 0.0 - 6.0 * eps
        and
        ((y + x/y) / 2.0) - (y + (x/y * (1.0 + eps)) * (1.0 + eps)) / 2.0 * (1.0 + eps) <= 6.0 * eps and
        ((y + x/y) / 2.0) - (y + (x/y * (1.0 + eps)) * (1.0 + eps)) / 2.0 * (1.0 + eps) >= 0.0 - 6.0 * eps
