cnf(eps, axiom, (eps = 2^(-23))).
cnf(up,axiom,(up(X) = (X)*(1+eps))).
cnf(dn,axiom,(dn(X) = (X)*(1-eps))).

fof(heron_preservation_total_i5,conjecture, 
  ! [X,Y] : 
  (
    (0.5 <= X & X <= 2 
    & 0.7 <= Y & Y <= 1.8
    & abs(sqrt(X) - Y) <= 0.5^(2^4) + 4*6*eps
    ) => 
    abs(sqrt(X) - up(up(Y + up(X/Y))/2)) <= 0.5^(2^5) + 5*6*eps &
    abs(sqrt(X) - dn(dn(Y + dn(X/Y))/2)) <= 0.5^(2^5) + 5*6*eps
  )).
