cnf(eps, axiom, (eps = 2^(-23))).
cnf(up,axiom,(up(X) = (X)*(1+eps))).
cnf(dn,axiom,(dn(X) = (X)*(1-eps))).

fof(heron_preservation_rounding,conjecture, 
  ! [X,Y] : 
  (
    (0.5 <= X & X <= 2 
    & 0.8 <= Y & Y <= 1.8
    ) => 
    abs((Y+X/Y)/2 - up(up(Y + up(X/Y))/2)) <= 0.1 &
    abs((Y+X/Y)/2 - dn(dn(Y + dn(X/Y))/2)) <= 0.1 
  )).
